{
  "type": "scaffold",
  "args": {
    "appBar": {
      "type": "app_bar",
      "args": {
        "title": {
          "type": "text",
          "args": {
            "text": "AnimatedPositioned"
          }
        }
      }
    },
    "body": {
      "type": "set_value",
      "args": {
        "values": {
          "customHeight": 40
        },
        "child": {
          "type": "stack",
          "args": {
            "children": [
              {
                "type" : "preferred_size",
                "args" : {
                  "preferredSize" : "${size}",
                  "child" : {
                    "type" : "container",
                    "args": {
                      "color" : "FF0000"
                    }
                  }
                }
              }
            ]
          }
        }
      }
    },
    "bottomNavigationBar" : {
      "type": "cupertino_tab_bar",
      "args": {
        "items": [
          {
            "bottomNavigationBarItem": {
              "type": "bottom_navigation_bar_item",
              "args": {
                "label": "Icon1",
                "icon": {
                  "type": "icon",
                  "args": {
                    "icon": {
                      "codePoint": 58712,
                      "fontFamily": "MaterialIcons"
                    }
                  }
                }
              }
            }
          },
          {
            "bottomNavigationBarItem": {
              "type": "bottom_navigation_bar_item",
              "args": {
                "label": "Icon2",
                "icon": {
                  "type": "icon",
                  "args": {
                    "icon": {
                      "codePoint": 984245,
                      "fontFamily": "MaterialIcons"
                    }
                  }
                }
              }
            }
          }
        ]
      }
    }
  }
}